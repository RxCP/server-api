{
  "private": true,
  "name": "server-api",
  "description": "",
  "scripts": {
    "build:app": "foal rmdir build/app && copy-cli \"server/**/*.html\" build && tsc -p tsconfig.app.json",
    "build:app:w": "tsc -p tsconfig.app.json -w",
    "start": "node ./build/index.js",
    "start:w": "supervisor -w ./build --no-restart-on error ./build/index.js",
    "develop": "npm run build:app && concurrently \"npm run build:app:w\" \"npm run start:w\"",
    "build:test": "foal rmdir build && copy-cli \"server/**/*.html\" build && tsc -p tsconfig.test.json",
    "build:test:w": "tsc -p tsconfig.test.json -w",
    "start:test": "mocha --file \"./build/test.js\" \"./build/**/*.spec.js\"",
    "start:test:w": "mocha --file \"./build/test.js\" -w \"./build/**/*.spec.js\"",
    "test": "npm run build:test && concurrently \"npm run start:test\"",
    "test:w": "npm run build:test && concurrently \"npm run build:test:w\" \"npm run start:test:w\"",
    "build:e2e": "foal rmdir build/e2e && copy-cli \"server/**/*.html\" build && tsc -p tsconfig.e2e.json",
    "build:e2e:w": "tsc -p tsconfig.e2e.json -w",
    "start:e2e": "mocha --file \"./build/e2e.js\" \"./build/e2e/**/*.js\"",
    "start:e2e:w": "mocha --file \"./build/e2e.js\" -w \"./build/e2e/**/*.js\"",
    "e2e": "npm run build:e2e && concurrently \"npm run build:e2e:w\" \"npm run start:e2e:w\"",
    "build:scripts": "foal rmdir build/scripts && tsc -p tsconfig.scripts.json",
    "build:scripts:w": "tsc -p tsconfig.scripts.json -w",
    "lint": "eslint --ext ts server",
    "lint:fix": "eslint --ext ts --fix server",
    "build:migrations": "foal rmdir build/migrations && tsc -p tsconfig.migrations.json",
    "migration:generate": "./node_modules/.bin/typeorm migration:generate",
    "migration:run": "./node_modules/.bin/typeorm migration:run",
    "migration:revert": "./node_modules/.bin/typeorm migration:revert",
    "format": "prettier --write \"server/**/*.ts\"",
    "init:env": "cp .env.example .env",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=8"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css,ts,tsx}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "@foal/core": "^1.0.0",
    "@foal/jwt": "^1.9.0",
    "@foal/typeorm": "^1.0.0",
    "class-validator": "^0.12.2",
    "copyfiles": "^2.3.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.1.0",
    "snyk": "^1.362.0",
    "source-map-support": "^0.5.1",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@foal/cli": "^1.9.0",
    "@foal/password": "^1.9.0",
    "@foal/typestack": "^1.9.0",
    "@snyk/graphlib": "^2.1.9-patch.2",
    "@snyk/lodash": "^4.17.15-patch",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@types/node": "^8.0.47",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "~2.7.0",
    "@typescript-eslint/parser": "~2.7.0",
    "chai": "^4.2.0",
    "class-transformer": "^0.3.1",
    "concurrently": "^3.5.1",
    "copy": "^0.3.2",
    "eslint": "^6.6.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^5.2.0",
    "prettier": "^2.0.5",
    "sqlite3": "^5.0.0",
    "supertest": "^3.3.0",
    "supervisor": "^0.12.0",
    "typescript": "~3.7.2"
  },
  "snyk": true
}
